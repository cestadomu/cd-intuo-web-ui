                    "worksheet.mergeCells('A'+data.length, 'C'+data.length);\r",
                    "\r",
                    "var pohotovostBezSvatku = ['Práce v pohotovosti bez sv.:', '', ''];\r",
                    "data.push(pohotovostBezSvatku);\r",
